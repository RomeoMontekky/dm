call print(--- negation ---)
neg(x, y) := !!(x->y)
call eval(neg)

call print(--- conjunction ---)
conj1(x) := 0 & x
call eval(conj1)
conj2(x) := 1 & x
call eval(conj2)
conj3(x, y) := (x->y) & (x->y) & x
call eval(conj3)

call print(--- disjunction ---)
disj1(x) := 0 | x
call eval(disj1)
disj2(x) := 1 | x
call eval(disj2)
disj3(x, y) := (x->y) | (x->y) | x
call eval(disj3)

