imp1(x) := (x + 1) -> 1 -> x 
call eval(imp1)
imp2(x) := 0 -> (x + 1) -> x
call eval(imp2)
imp3(x) := (x + 1) -> (x + 1) -> x
call eval(imp3)
imp4(x, y) := !(x + y) -> 0
call eval(imp4)
imp5(x, y) := !(x + y) -> (x + y)
call eval(imp5)
imp5a(x, y) := (x + y + 1) -> (x + y)
call eval(imp5a)
imp6(x, y) := (x = y) -> !(x = y)
call eval(imp6)
imp6a(x, y) := (x = y) -> (y = 0 = x)
call eval(imp6a)
imp7(x) := (x + 1) -> 0 -> 0 -> x
call eval(imp7)
# in-place normalization
imp8(x) := !(x->0) -> 0 -> 0
call eval(imp8)
# grouping, reduction (x -> x -> y) => y, in-place normalization
imp9(x, y) := x -> y -> 0 -> (x -> y -> 0) -> (x -> 0) -> 0
call eval(imp9)
# grouping, reduction (x -> !x) => !x
imp10(x) := (x + 1) -> x -> !((x + 1) -> x)
call eval(imp10)
# (x -> y -> x) => (x). Without grouping.
imp11(x) := (x + 1) -> x -> (x + 1)
call eval(imp11)
# (x -> y -> x) => (x). With grouping.
imp12(x, y, z) := x -> y -> z -> (x + y + z) -> (x -> y -> z)
call eval(imp12)
# multilevel evaluation/normalization/simplification
imp13(x) := x -> (!((x + 1) -> 1 -> (x -> 0)) -> (x -> 0) -> 0)
call eval(imp13)
# absense of commutativity
imp14(x, y) := (x -> y) -> (y -> x)
call eval(imp14)