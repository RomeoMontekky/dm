--- negation ---
neg(x, y) := !!(x -> y)
neg(x, y) := (x -> y)
--- conjunction ---
conj1(x) := (x & 0)
conj1(x) := 0
conj2(x) := (x & 1)
conj2(x) := x
conj3(x, y) := ((x -> y) & (x -> y) & x)
conj3(x, y) := ((x -> y) & x)
conj4(x) := ((x + 1) & x & !(x + 1))
conj4(x) := 0
--- disjunction ---
disj1(x) := (x | 0)
disj1(x) := x
disj2(x) := (x | 1)
disj2(x) := 1
disj3(x, y) := ((x -> y) | (x -> y) | x)
disj3(x, y) := ((x -> y) | x)
disj4(x) := ((x + 1) | x | !(x + 1))
disj4(x) := 1
--- implication ---
imp1(x) := ((x + 1) -> 1 -> x)
imp1(x) := x
imp2(x) := (0 -> (x + 1) -> x)
imp2(x) := x
imp3(x) := ((x + 1) -> (x + 1) -> x)
imp3(x) := x
imp4(x) := (!(x + 1) -> 0)
imp4(x) := (x + 1)
imp5(x) := (!(x + 1) -> (x + 1))
imp5(x) := (x + 1)
imp6(x) := ((x + 1) -> !(x + 1))
imp6(x) := !(x + 1)
imp7(x) := ((x + 1) -> 0 -> 0 -> x)
imp7(x) := ((x + 1) -> x)
imp8(x) := (!(x -> 0) -> 0 -> 0)
imp8(x) := x
imp9(x, y) := (x -> y -> 0 -> (x -> y -> 0) -> (x -> 0) -> 0)
imp9(x, y) := x
imp10(x) := ((x + 1) -> x -> (x + 1))
imp10(x) := (x + 1)
imp11(x, y, z) := (x -> y -> z -> (x + y + z) -> (x -> y -> z))
imp11(x, y, z) := (x -> y -> z)
--- equality ---
eq1(x) := (x = 1)
eq1(x) := x
eq2(x) := (x = 0)
eq2(x) := (x = 0)
eq3(x) := (x = x)
eq3(x) := 1
eq4(x, y) := (!x = !y)
eq4(x, y) := (x = y)
eq5(x, y, z) := (z = !(x -> y) = (x -> y) = 0)
eq5(x, y, z) := z
eq6(x) := ((x + 1) = !(x + 1))
eq6(x) := 0
eq7(x, y) := (!(x = 0) = !(y = 0))
eq7(x, y) := (x = y)
--- plus ---
plus1(x) := (x + 1)
plus1(x) := (x + 1)
plus2(x) := (x + 0)
plus2(x) := x
plus3(x) := (x + x)
plus3(x) := 0
plus4(x, y) := (!x + !y)
plus4(x, y) := (x + y)
plus5(x, y, z) := (z + !(x -> y) + (x -> y) + 1)
plus5(x, y, z) := z
plus6(x) := ((x = 0) + !(x = 0))
plus6(x) := 1
plus7(x, y) := (!(x + 1) + !(y + 1))
plus7(x, y) := (x + y)
plus8(x1, x2, x3, x4, x5, x6) := (x1 + x2 + x6 + x2 + x1 + x3 + x4 + x4 + x5 + x6)
plus8(x1, x2, x3, x4, x5, x6) := (x3 + x5)
