conj1(x) := (x & 0)
conj1(x) := 0
conj2(x) := (x & 1)
conj2(x) := x
conj3(x, y) := ((x -> y) & (x -> y) & x)
conj3(x, y) := ((x -> y) & x)
conj4(x, y) := ((x + y) & x & !(x + y))
conj4(x, y) := 0
conj5(x) := (x & (x = 0))
conj5(x) := 0
conj6(x, y) := ((x + y) & x & (y + x + 1))
conj6(x, y) := 0
conj7(x, y, z) := (!(x & y) & y & z & x)
conj7(x, y, z) := 0
